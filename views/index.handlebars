<script src="app.js"></script>
<link rel="stylesheet" type="text/css" href="style.css"/>

<h1>Welcome to Your News Scraper!</h1>

{{#each data}}

    <div id="wrapper">
    <p data-id={{this.id}} title={{this.title}} class="title">Article: {{this.title}}</p>
    <p data-id={{this.id}} title={{this.title}}>Summary: {{this.summary}}</p>
    <p data-id={{this.id}} title={{this.title}}>Link: {{this.link}}</p>
    <div id="titles"></div>
    <div id="summaries"></div>
    <div id="links"></div>
</div>

{{/each}}


{{#each note}}
    <p data-id={{this.id}}>{{this.title}}</p>
    <p data-id={{this.id}}>{{this.body}}</p>
{{/each}}